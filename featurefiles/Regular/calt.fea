lookup Q_longtail {
    sub Q' @NO_DESC @NO_DESC by Q.long;
    sub Q' @NO_DESC [ @NO_DESC quoteleft quoteright ] @RIGHT_DESC  by Q.long;
} Q_longtail;
lookup f_long {
    sub @SHORT_F' [ @NO_ASC ] by @LONG_F;
} f_long;
lookup caltLAT {
    #sub [ space @PUNCT ] u.LAT' by u.LATinit;
    sub [ @Letters U.LAT u.LAT u.LATmedi v.LATmedi ] [ u.LAT v.LAT]' by [ u.LATmedi v.LATmedi];
} caltLAT;

feature calt {
    lookup Q_longtail;
    lookup f_long;
    lookup caltLAT;
} calt;
