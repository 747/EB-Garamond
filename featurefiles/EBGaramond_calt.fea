lookup Q_longtail {
    sub Q' @NO_DESC @NO_DESC by Q.long;
    sub Q' @NO_DESC @NO_DESC @RIGHT_DESC  by Q.long;
} Q_longtail;
lookup f_short {
    sub @LONG_F' [ @HIGH_ASC space @PUNCT ] by @SHORT_F;
    sub @LONG_F' \uni200c @HIGH_ASC by @SHORT_F;
} f_short;

feature calt {
    lookup Q_longtail;
    lookup f_short;
} calt;
